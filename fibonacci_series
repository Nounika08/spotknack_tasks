public class Main {
    public static void main(String[] args) {


        int first = 0, second = 1;

        System.out.println("Fibonacci Series up to " + 25 + " terms:");

        for (int i = 1; i <=25 ; i++) {
            System.out.print(first + " ");
            int next = first + second;
            first = second;
            second = next;
        }
    }
}